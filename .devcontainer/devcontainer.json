{
    "name": "Development Container",
    "dockerComposeFile": "compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "remoteUser": "vscode",
    // "forwardPorts": [3000, 8080],
    "postCreateCommand": "rustc --version && cargo --version && node --version && npm --version",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "vadimcn.vscode-lldb",
                "serayuzgur.crates",
                "tamasfe.even-better-toml"
            ]
        }
    }
}
